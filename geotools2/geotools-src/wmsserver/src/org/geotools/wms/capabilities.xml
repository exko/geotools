<!DOCTYPE WMT_MS_Capabilities SYSTEM "http://www.digitalearth.gov/wmt/xml/capabilities_1_1_1.dtd"> 

<!-- This is a template file of the Capabilities XML Tags surrounded by <?GEO ?> are replaced with relevent information by the WMSServlet -->

<WMT_MS_Capabilities version="1.1.1" updateSequence="0">

<Service>
  <Name>OGC:WMS</Name> 
  <Title>GeoTools WMS</Title> 
  <Abstract>WMS 1.1.1 Compatible GeoTools Map Server</Abstract> 

  <KeywordList>
    <Keyword>GeoTools</Keyword> 
  </KeywordList>

  <OnlineResource xmlns:xlink="http://www.geotools.org" xlink:type="simple" xlink:href="http://www.geotools.org/" /> 
  <ContactInformation>
    <ContactPersonPrimary>
      <ContactPerson>James McGill</ContactPerson> 
      <ContactOrganization>Leeds University</ContactOrganization> 
    </ContactPersonPrimary>
    <ContactPosition>Project Lead</ContactPosition> 
    <ContactElectronicMailAddress>j.macgill@geography.leeds.ac.uk</ContactElectronicMailAddress> 
  </ContactInformation>

  <Fees>none</Fees> 
  <AccessConstraints>none</AccessConstraints> 
</Service>

<Capability>

<Request>

<GetCapabilities>
  <Format>application/vnd.ogc.wms_xml</Format> 
  <DCPType>
    <HTTP>
      <Get></Get>
    </HTTP>
  </DCPType>
</GetCapabilities>

<GetMap>
  <Format>image/jpeg</Format> 
  <?GEO MAPFORMATS ?>
  <DCPType>
    <HTTP>
      <Get>
         <OnlineResource xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="http://www.ccg.leeds.ac.uk:8080/GT2WMS/servlet/GT2WMS?"/>
      </Get>
    </HTTP>
  </DCPType>
</GetMap>

<?GEO GETFEATUREINFO ?>

</Request>

<Exception>
  <Format>application/vnd.ogc.se_xml</Format> 
  <?GEO EXCEPTIONFORMATS ?>
</Exception>

<VendorSpecificCapabilities> 
  <?GEO VENDORSPECIFICCAPABILITIES ?>
</VendorSpecificCapabilities>

<!-- Layers -->

<?GEO LAYERS ?>

</Capability>
</WMT_MS_Capabilities>