<WMSServer>
	<Layer id="USA" srs="EPSG:4326">
		<datasource class="Shapefile">
			<param name="url" value="maps/statepop.shp"/>
		</datasource>
		<description>USA</description>
		<Style id="thick outline" filename="styles/polyshp.sld"/>
		<Style id="dotty" filename="styles/funkypolyshp.sld"/>
		<Style id="green" filename="styles/green.sld"/>
		<Style id="population" filename="styles/popshade.sld"/>
		<Style id="highlight" filename="styles/usa_penn.sld"/>
		<Style id="bbox" filename="styles/usa_central.sld"/>
		<Style id="maleVsFemale" filename="styles/usa_ratio.sld"/>
		<Style id="normal" filename="styles/normal.sld" default="true"/>
		<Style id="manualVsWorkers" filename="styles/usa_math.sld"/>
	</Layer>
        <Layer id="USAmif" srs="EPSG:4326">
		<datasource>
			<param name="url" value="maps/statepop.mif"/>
		</datasource>
		<description>USA from a MIF/MID</description>
		<Style id="thick outline" filename="styles/polyshp.sld"/>
		<Style id="dotty" filename="styles/funkypolyshp.sld"/>
		<Style id="green" filename="styles/green.sld"/>
		<Style id="population" filename="styles/popshade.sld"/>
		<Style id="highlight" filename="styles/usa_penn.sld"/>
		<Style id="bbox" filename="styles/usa_central.sld"/>
		<Style id="maleVsFemale" filename="styles/usa_ratio.sld"/>
		<Style id="normal" filename="styles/normal.sld" default="true"/>
		<Style id="manualVsWorkers" filename="styles/usa_math.sld"/>
	</Layer>
	<Layer id="ShpForest" srs="EPSG:4326">
		<datasource class="Shapefile">
			<param name="url" value="maps/Forests.shp"/>
		</datasource>
		<description>USA</description>
		<Style id="green" filename="styles/green.sld"/>
	</Layer>
        <Layer id="ShpBuildings" srs="EPSG:4326">
		<datasource class="Shapefile">
			<param name="url" value="maps/Buildings.shp"/>
		</datasource>
		<description>USA</description>
		<Style id="green" filename="styles/green.sld"/>
	</Layer>

