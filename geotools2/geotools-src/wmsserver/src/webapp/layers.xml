<?xml version="1.0" encoding="UTF-8"?>

<!-- Created by jamesm on 15 August 2002, 12:47 -->

<WMSServer>
    <Layer id="USA" srs="EPSG:4326">
        <datasource class="Shapefile">
            <param name="filename" value="maps/statepop.shp"/>
        </datasource>
        <description>The USA</description>
        <Style id="thick outline" filename="styles/polyshp.sld"/>
        <Style id="dotty" filename="styles/funkypolyshp.sld"/>
        <Style id="population" filename="styles/popshade.sld"/>
        <Style id="highlight" filename="styles/usa_penn.sld"></Style>
        <Style id="bbox" filename="styles/usa_central.sld"></Style>
        <Style id="male vs female" filename="styles/usa_ratio.sld"></Style>
        <Style id="normal" filename="styles/normal.sld" default="true"></Style>
        <Style id="employ/persons" filename="styles/usa_math.sld"></Style>
    </Layer>
  
    <Layer id="postgistest">
        <datasource class="PostGIS">
            <param name="host" value="your.host.com"></param>
            <param name="user" value="username"/>
            <param name="passwd" value="password"/>
            <param name="port" value="5432"/>
            <param name="table" value="tablename"/>
            <param name="database" value="database"/>
        </datasource>
        <Style id="normal" filename="styles/normal.sld" default="true"></Style>
        <description>PostGIS Connection</description>
   </Layer>
</WMSServer>

