<Filters>
<Filter xmlns:gml="http://www.opengis.net/gml">
    <Not>
        <Disjoint>
            <PropertyName>testGeometry</PropertyName>
            <Literal>
                <gml:Box srsName="http://www.epsg.com">
                    <gml:coordinates>0,0 10,10</gml:coordinates>
                </gml:Box>
            </Literal>
        </Disjoint>
    </Not>
</Filter>
<Filter  xmlns:gml="http://www.opengis.net/gml">
    <BBOX>
        <PropertyName>testGeometry</PropertyName>
        <Literal>
            <gml:Box srsName="http://www.epsg.com">
                <gml:coordinates>0,0 10,10</gml:coordinates>
            </gml:Box>
        </Literal>
    </BBOX>
</Filter>
</Filters>